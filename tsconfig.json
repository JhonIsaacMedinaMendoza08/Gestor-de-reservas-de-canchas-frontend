{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/courts/page.jsx", "src/app/reservations/page.jsx", "src/app/error.jsx", "src/components/CourtForm.jsx", "src/components/CourtsTable.jsx", "src/components/Navbar.jsx", "src/components/ReservationForm.jsx", "src/components/ReservationsTable.jsx", "src/components/Toast.jsx", "src/lib/api.js", "src/lib/services/courts.js", "src/lib/services/reservations.js", "tailwind.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}
